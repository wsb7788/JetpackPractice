<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="335dp"
    android:layout_height="104dp"
    android:background="@drawable/bg_chat_dialog"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/chat_dialog_report_type_btn"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center"
        android:text="@string/chat_dialog_report"
        android:textSize="16sp"
        android:textColor="@color/dark_gray"
        android:fontFamily="@font/notosanskr_regular"
        android:clickable="true"
        android:focusable="true" />

    <TextView
        android:id="@+id/chat_dialog_expel_type_btn"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        app:layout_constraintTop_toBottomOf="@+id/chat_dialog_report_type_btn"
        android:gravity="center"
        android:text="@string/chat_dialog_expel"
        android:textSize="16sp"
        android:textColor="@color/dark_gray"
        android:fontFamily="@font/notosanskr_regular"
        android:clickable="true"
        android:focusable="true"/>
</androidx.constraintlayout.widget.ConstraintLayout>